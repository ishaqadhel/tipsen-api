{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "ignorePatterns": [""],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "simple-import-sort", "unused-imports"],
  "rules": {
    "no-unused-vars": "off",
    "no-console": "warn"
  }
}
